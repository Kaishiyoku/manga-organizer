  "bugs": {
    "url": "https://github.com/Kaishiyoku/manga-organizer/issues"
  },
  "homepage": "https://github.com/Kaishiyoku/manga-organizer#readme",
  "devDependencies": {
    "bootstrap": "4.0.0",
    "laravel-mix": "^2.1.0"
  },
  "dependencies": {
    "cross-env": "^5.1.4"
