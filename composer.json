{
  "name": "kaishiyoku/manga-organizer",
  "description": "Simple manga organizer.",
  "keywords": [
    "framework",
    "laravel",
    "manga"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2",
    "albertcht/invisible-recaptcha": "1.9.4",
    "doctrine/dbal": "2.10.2",
    "fideloper/proxy": "4.3.0",
    "fruitcake/laravel-cors": "2.0.1",
    "guzzlehttp/guzzle": "6.5.4",
    "jikan-me/jikan": "2.16.5",
    "kaishiyoku/laravel-cli-create-user": "2.3.3",
    "kaishiyoku/laravel-menu": "4.4.5",
    "laracasts/flash": "3.1",
    "laravel/framework": "7.14.1",
    "laravel/tinker": "2.4.0",
    "laravel/ui": "2.0.3",
    "laravelcollective/html": "6.1.2",
    "spatie/laravel-backup": "6.10.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "3.3.3",
    "barryvdh/laravel-ide-helper": "2.7.0",
    "facade/ignition": "2.0.7",
    "fzaninotto/faker": "1.9.1",
    "mockery/mockery": "1.4.0",
    "nunomaduro/collision": "4.2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan ide-helper:models --dir=\"app/Models\" --write"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
