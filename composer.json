{
  "name": "kaishiyoku/manga-organizer",
  "description": "Simple manga organizer.",
  "keywords": [
    "framework",
    "laravel",
    "manga"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "albertcht/invisible-recaptcha": "1.8.3",
    "doctrine/dbal": "2.7.2",
    "fideloper/proxy": "4.0.0",
    "jikan-me/jikan": "2.0.0-rc.1",
    "kaishiyoku/laravel-menu": "2.0.0",
    "kaishiyoku/yaml-translation": "5.6.0",
    "laracasts/flash": "3.0.2",
    "laravel/framework": "5.6.33",
    "laravel/tinker": "1.0.7",
    "laravelcollective/html": "5.6.10",
    "spatie/laravel-backup": "5.10.0",
    "webup/laravel-sendinblue": "1.1.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "3.1.5",
    "barryvdh/laravel-ide-helper": "2.4.3",
    "filp/whoops": "2.2.0",
    "fzaninotto/faker": "1.8.0",
    "mockery/mockery": "1.1.0",
    "nunomaduro/collision": "2.0.3",
    "phpunit/phpunit": "7.3.1"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan ide-helper:models --dir=\"app/Models\" --write"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

